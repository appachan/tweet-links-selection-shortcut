{
  "name": "tweet-links-selection-shortcut",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content_script.bundle.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
